<div class="contents row">
  <div class="container">
    <div class="post">
    <h3>投稿する</h3>
    <%= form_with(model: @post, local: true) do |f| %>
      <%= f.file_field :image, placeholder: "Image Url" %>
      <%= f.text_area :content, placeholder: "メッセージ", rows: "10" %>
      <%= f.submit "SEND" %>
    <% end %>
    </div>
  </div>
</div>